{
    "name": "goodtogo_backend",
    "version": "0.0.1",
    "description": "GoodToGo Backend Server",
    "main": "index.js",
    "scripts": {
        "googleDebug": "echo \"Debudding Mode\" & set DEBUG=goodtogo_backend:* & set PORT=3030 & nodemon bin/wwwhttp",
        "socketDebug": "echo \"Debudding Mode\" & set DEBUG=goodtogo_backend:*,socket.io:* & set PORT=3030 & set NODE_ENV=\"testing\" & nodemon bin/wwwhttp",
        "redisDebug": "echo \"Debudding Mode\" & set DEBUG=goodtogo_backend:* & set NODE_ENV=testing & set NODE_DEBUG=redis & nodemon bin/wwwhttp",
        "localTest": "echo \"Debudding Mode\" & set DEBUG=goodtogo_backend:* & set PORT=3030 & set NODE_ENV=\"testing\" & nodemon bin/wwwhttp",
        "test": "echo \"Debudding Mode\" & set DEBUG=goodtogo_backend:* & nodemon bin/www",
        "start": "nodemon bin/www --ignore tmp/"
    },
    "nodemonConfig": {
        "ignore": [
            "assets/*",
            "*.json",
            "tmp/*"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/BenjaminChenTW/GoodToGo_Backend.git"
    },
    "author": "BenjaminChenTW",
    "license": "ISC",
    "dependencies": {
        "aws-sdk": "^2.290.0",
        "bcrypt-nodejs": "0.0.3",
        "body-parser": "^1.18.3",
        "buffer-crc32": "^0.2.13",
        "connect-timeout": "^1.9.0",
        "cookie-parser": "~1.4.3",
        "crypto": "^1.0.1",
        "debug": "^3.1.0",
        "ejs": "~2.5.7",
        "express": "^4.15.4",
        "express-status-monitor": "^1.0.1",
        "google-auth-library": "https://registry.npmjs.org/google-auth-library/-/google-auth-library-0.12.0.tgz",
        "googleapis": "^23.0.0",
        "helmet": "^3.9.0",
        "jwt-simple": "^0.5.1",
        "mongoose": "^4.13.7",
        "morgan": "^1.9.0",
        "on-finished": "^2.3.0",
        "on-headers": "^1.0.1",
        "redis": "^2.8.0",
        "serve-favicon": "~2.4.3",
        "sharp": "^0.18.4",
        "socket.io": "^2.1.0",
        "universal-analytics": "^0.4.16"
    },
    "bugs": {
        "url": "https://github.com/BenjaminChenTW/GoodToGo_Backend/issues"
    },
    "homepage": "https://github.com/BenjaminChenTW/GoodToGo_Backend#readme"
}

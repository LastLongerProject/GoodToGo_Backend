{
    "name": "goodtogo_backend",
    "version": "0.0.1",
    "description": "GoodToGo Backend Server",
    "main": "index.js",
    "scripts": {
        "googleDebug": "echo \"Debudding Mode\" & set DEBUG=goodtogo_backend:* & set PORT=3030 & nodemon app.js",
        "socketDebug": "echo \"Debudding Mode\" & set DEBUG=goodtogo_backend:*,socket.io:* & set PORT=3030 & set NODE_ENV=\"testing\" & nodemon app.js",
        "redisDebug": "echo \"Debudding Mode\" & set DEBUG=goodtogo_backend:* & set NODE_ENV=testing & set NODE_DEBUG=redis & nodemon app.js",
        "nodemonDebug": "echo \"Debudding Mode\" & set DEBUG=goodtogo_backend:* & set PORT=3030 & set NODE_ENV=\"testing\" & nodemon app.js --verbose",
        "ngrok": "ngrok http -region ap 3030",
        "test": "echo \"Debudding Mode\" && DEBUG=goodtogo_backend:* && PORT=3030 && NODE_ENV=\"testing\" && nodemon app.js",
        "start": "nodemon app.js --ignore tmp/",
        "unitTest": "mocha"
    },
    "nodemonConfig": {
        "ignore": [
            "assets/*",
            "*.json",
            "tmp/*"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/BenjaminChenTW/GoodToGo_Backend.git"
    },
    "author": "BenjaminChenTW",
    "license": "ISC",
    "dependencies": {
        "@lastlongerproject/toolkit": "^1.0.2",
        "apidoc": "^0.17.7",
        "aws-sdk": "^2.296.0",
        "bcrypt-nodejs": "0.0.3",
        "body-parser": "^1.18.3",
        "buffer-crc32": "^0.2.13",
        "connect-timeout": "^1.9.0",
        "cookie-parser": "~1.4.3",
        "crypto": "^1.0.1",
        "debug": "^3.1.0",
        "ejs": "~2.5.7",
        "express": "^4.15.4",
        "express-status-monitor": "^1.0.1",
        "googleapis": "^32.0.0",
        "helmet": "^3.9.0",
        "jwt-simple": "^0.5.1",
        "mongoose": "^4.13.17",
        "morgan": "^1.9.1",
        "on-finished": "^2.3.0",
        "on-headers": "^1.0.1",
        "queue": "^4.5.0",
        "redis": "^2.8.0",
        "serve-favicon": "~2.4.3",
        "sharp": "^0.18.4",
        "socket.io": "^2.1.0",
        "universal-analytics": "^0.4.20",
        "uuid": "^3.3.2"
    },
    "bugs": {
        "url": "https://github.com/BenjaminChenTW/GoodToGo_Backend/issues"
    },
    "homepage": "https://github.com/BenjaminChenTW/GoodToGo_Backend#readme",
    "devDependencies": {
        "chai": "^4.2.0",
        "mocha": "^5.2.0",
        "should": "^13.2.3",
        "supertest": "^3.3.0"
    }
}
